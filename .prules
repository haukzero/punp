# Rules:
# - Comment: # This is a comment
# - Add rule: "old text" -> "new text"
# - Delete rule: - "old text"
# - Protected region: !"start" ~ "end"
# - Clear rules: --

# Default rules
"。" -> ". "
"，" -> ", "
"、" -> ", "
"；" -> "; "
"？" -> "? "
"！" -> "! "
"：" -> ": "
"“" -> """
"”" -> """
"‘" -> "'"
"’" -> "'"
"（" -> "("
"）" -> ")"
"【" -> "["
"】" -> "]"

# Add your new rules here, the new will overwrite the old
